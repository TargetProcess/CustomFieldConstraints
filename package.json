{
  "name": "targetprocess-mashup-webpack-boilerplate",
  "version": "0.1.0",
  "description": "Webpack boilerplate to create Targetprocess mashup.",
  "keywords": [
    "webpack",
    "targetprocess",
    "mashup"
  ],
  "author": "Aliaksei Shytkin <shitkin@targetprocess.com>",
  "scripts": {
    "start": "rm -rf $TARGETPROCESS_MASHUPS_PATH/CustomFieldConstraints && NODE_ENV=development webpack --watch --output-path $TARGETPROCESS_MASHUPS_PATH/CustomFieldConstraints",
    "build": "webpack",
    "build-production": "webpack --output-path build --config webpack-production.config.js && mv build/index.js build/release.is",
    "build-library": "rm -rf build/library && webpack --output-path build/library --config webpack-library.config.js && cp docs/README.md build/library/README.mkd && cp docs/*.png build/library",
    "test": "eslint --quiet ./src && karma start --single-run"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/mit-license.php"
    }
  ],
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.1.3",
    "bluebird": "^3.0.5",
    "chai": "^3.4.0",
    "classnames": "^2.2.0",
    "combine-assets-plugin": "targetprocess/combine-assets-plugin",
    "css-loader": "^0.14.5",
    "eslint": "^1.7.3",
    "eslint-plugin-babel": "^2.1.1",
    "eslint-plugin-mocha": "^1.0.0",
    "eslint-plugin-react": "^3.6.3",
    "file-loader": "^0.8.4",
    "karma": "^0.13.14",
    "karma-chai": "^0.1.0",
    "karma-chai-plugins": "^0.6.0",
    "karma-chrome-launcher": "^0.2.1",
    "karma-cli": "^0.1.1",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-spec-reporter": "0.0.21",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.3",
    "null-loader": "^0.1.1",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "react": "^0.13.3",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.12.3",
    "targetprocess-mashup-config-loader": "targetprocess/targetprocess-mashup-config-loader",
    "targetprocess-mashup-helper": "^0.3.0",
    "targetprocess-mashup-manifest-loader": "targetprocess/targetprocess-mashup-manifest-loader",
    "targetprocess-mashup-webpack-plugin": "targetprocess/targetprocess-mashup-webpack-plugin",
    "underscore": "^1.8.3",
    "webpack": "^1.9.10"
  },
  "dependencies": {}
}
